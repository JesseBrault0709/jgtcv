<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

  <Appenders>

    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout>
        <Pattern>[%thread] %level %logger{1} %method %line: %message%n</Pattern>
      </PatternLayout>
    </Console>

  </Appenders>

  <Loggers>

    <Root level="trace">
      <AppenderRef ref="Console"/>
    </Root>
    
  </Loggers>
  
</Configuration>